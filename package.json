{
  "name": "Battles Demo",
  "version": "0.0.1",
  "description": "Demo",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Ankit Kumar Gaur",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "csvtojson": "^2.0.8",
    "express": "^4.13.3",
    "jsonwebtoken": "^7.4.3",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.1.1",
    "jest": "^21.0.2",
    "nodemon": "^1.9.2",
    "winston": "^2.3.1"
  }
}
